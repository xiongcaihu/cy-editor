{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2015",
    "lib": ["es5", "dom", "ES2015"],
    "types": ["cypress"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "noEmit": true,
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
